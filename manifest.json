{
  "manifest_version": 2,
  "name": "Tribal",
  "version": "0.1.1",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://tribunnews.com/*",
        "*://*.tribunnews.com/*",
        "*://grid.id/*",
        "*://*.grid.id/*"
      ],
      "js": ["index.js"]
    }
  ]
}